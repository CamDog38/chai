{
  "version": 2,
  "cleanUrls": true,
  "trailingSlash": false,
  "redirects": [
    { "source": "/", "destination": "/chai/", "permanent": false }
  ],
  "headers": [
    {
      "source": "/chai/Images/(.*)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    },
    {
      "source": "/(.*)\\.(js|css|png|jpg|jpeg|gif|webp|svg)",
      "headers": [
        { "key": "Cache-Control", "value": "public, max-age=31536000, immutable" }
      ]
    }
  ],
  "rewrites": [
    { "source": "/index", "destination": "/chai/index.html" },
    { "source": "/index.html", "destination": "/chai/index.html" },
    { "source": "/mediaportfolio", "destination": "/chai/mediaportfolio.html" },
    { "source": "/mediaportfolio.html", "destination": "/chai/mediaportfolio.html" },
    { "source": "/sportmediasportfolio", "destination": "/chai/sportmediasportfolio.html" },
    { "source": "/sportmediasportfolio.html", "destination": "/chai/sportmediasportfolio.html" }
  ]
}
